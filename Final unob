local bidd = bank:AddDropdown("Store Item", function(val)
islot = val
end)

bidd:Add("Slot 1")
bidd:Add("Slot 2")
bidd:Add("Slot 3")
bidd:Add("black 4")
bidd:Add("black 5")
bidd:Add("black 6")

