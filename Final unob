local bi = Window:AddTab("B")

local pb = ""

local budd = bi:AddDropdown("Buy Item",function(val)
pb = val
end)

local items =
{
"Arrow",
"Pizza",
"Rokakaka Fruit",
"Umbrella Hat"
}

for _,v in pairs(items) do
budd:Add(v)
end

bi:AddButton("Buy Item",function()

if pb == "Arrow" then

local args = {
    [1] = "Arrow"
}

game:GetService("ReplicatedStorage").Main.Shop.Buy:FireServer(unpack(args))

elseif pb == "Pizza" then

local args = {
    [1] = "Pizza"
}

game:GetService("ReplicatedStorage").Main.Shop.Buy:FireServer(unpack(args))

elseif pb == "Rokakaka Fruit" then

