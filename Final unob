--farming

f:AddSwitch("Auto Map Items",function(state)

if state then

        _G.on = true
    
        while _G.on do
            wait()
            
             for _, v in pairs(game.workspace:GetDescendants()) do

if v:IsA("TouchTransmitter") then

if v.Parent.Name == "Handle" then

firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)

wait()

firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)

end

end

end


        end 
    else
        _G.on = false
    end
end)

f:AddSwitch("Auto Quest",function(state)

if state then

        _G.on = true
    
        while _G.on do
            task.wait()


             local args = {
    [1] = "Gucci",
    [2] = false
}

game:GetService("ReplicatedStorage").Main.Shop.Quest:FireServer(unpack(args))

local args = {
    [1] = "Turtle",
    [2] = false
}

game:GetService("ReplicatedStorage").Main.Shop.Quest:FireServer(unpack(args))

local args = {
    [1] = "Letter",
    [2] = true 
}

game:GetService("ReplicatedStorage").Main.Shop.Quest:FireServer(unpack(args))

local args = {
    [1] = "Turtle",
    [2] = true
}

game:GetService("ReplicatedStorage").Main.Shop.Quest:FireServer(unpack(args))

local args = {
    [1] = "Gucci",
    [2] = true
}

game:GetService("ReplicatedStorage").Main.Shop.Quest:FireServer(unpack(args))


        end 
    else
        _G.on = false
    end
end)

f:AddButton("Use Items",function()

for _, v in pairs(game:GetService("Players").LocalPlayer:FindFirstChildOfClass("Backpack"):GetChildren()) do

